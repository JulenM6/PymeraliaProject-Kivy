#:kivy 2.1.0
<DetailsReportScreen>:
    MDTopAppBar:
        title: "PymeShield"
        elevation: 1
        pos_hint: {"top": 1}
        md_bg_color: "#ff6720"
        specific_text_color: "black"
        left_action_items: [["keyboard-backspace", lambda x: root.goBack('report')]]
    MDBoxLayout:
        padding:dp(0), dp(50), dp(0), dp(0)
        orientation: "vertical"
        MDLabel:
            id: estatext
            text:""
            halign: "center"
            font_style: "H5"
        Image:
            id: imagen
            center: self.parent.center
            size: 600, 600
        MDScrollView:
            MDList:
                id: estat